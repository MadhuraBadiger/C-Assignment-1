//code for Relational operators 
#include<stdio.h>
int main() {
     int a,b;
     a=13;
     b=17;
     //operation for lesser//
     printf("a<b=%d\n",a<b);
     //operation for greater//
     printf("a>b=%d\n",a>b);
     //operation for lesserthan//
     printf("a<=b=%d\n",a<=b);
     //operation for greaterthan//
     printf("a>=b=%d\n",a>=b);
     //operation for equalto//
     printf("a=b=%d\n",a=b);
     //operation for double equal to//
     printf("a==b=%d\n",a==b);
     //operation for not equal to//
     printf("a!=b=%d\n",a!=b);
     return 0;
     }
     
     
    Output:
    a<b=1
    a>b=0
    a<=b=1
    a>=b=0
    a=b=17
    a==b=1
    a!=b=0